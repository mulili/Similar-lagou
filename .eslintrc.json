{
    "extends": "airbnb",
    "plugins": [
      "react"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jquery": true
    },
    "parser": "babel-eslint",
    "rules": {
      "react/jsx-filename-extension": [1, {
        "extensions": [".js", ".jsx"]
      }],
      "import/no-extraneous-dependencies": 0,
      "comma-dangle": [2, "never"],
      "no-unused-vars":1,
      "no-debugger":1,
      "no-underscore-dangle":0
    }
  }